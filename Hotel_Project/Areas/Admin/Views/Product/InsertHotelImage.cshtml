@using Hotel_Project.ViewModels.Product.HotelImage;
@model InsertAndRemoveImage
@{
    ViewData["Title"] = "InsertHotelImage";
}

 <div class="container">
     <h3 class="py-3"> افزودن تصویر هتل</h3>
    <form asp-action="InsertHotelImage" method="post" enctype="multipart/form-data">
        <input asp-for="Id" type="hidden" />
        <div class="row">
            <div class="mb-3 col-6">
                <label asp-for=File  class="col-form-label"></label>
                <input asp-for="File" class="form-control" />
                <span asp-validation-for="File" class="text-bg-danger"></span>
            </div>
            <div class="mb-3" >
                <button type="submit"class="btn btn-outline-primary">افزودن</button>
            </div>

        </div>

    
    
    </form>

 </div>
